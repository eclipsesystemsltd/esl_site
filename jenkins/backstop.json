{
  "id": "backstop_default",
  "viewports": [
    {
      "label": "desktop",
      "width": 1600,
      "height": 1200
    },
    {
      "label": "tablet",
      "width": 1600,
      "height": 1200
    }
  ],
  "onBeforeScript": "puppet/onBefore.js",
  "onReadyScript": "puppet/onReady.js",
  "scenarios": [
    {
      "label": "BackstopJS Homepage",
      "cookiePath": "backstop_data/engine_scripts/cookies.json",
      "url": "http://www.eclipsesystems.co.uk",
      "referenceUrl": "http://localhost/~graham",
      "readyEvent": "",
      "readySelector": "",
      "delay": 1000,
      "hideSelectors": [],
      "removeSelectors": [],
      "hoverSelector": "#contactus",
      "clickSelector": "",
      "postInteractionWait": 10,
      "selectors": [],
      "selectorExpansion": true,
      "expect": 0,
      "misMatchThreshold" : 0.1,
      "requireSameDimensions": true
    }
  ],
  "paths": {
    "bitmaps_reference": "jenkins/backstop_data/bitmaps_reference",
    "bitmaps_test": "jenkins/backstop_data/bitmaps_test",
    "engine_scripts": "jenkins/backstop_data/engine_scripts",
    "html_report": "jenkins/backstop_data/html_report",
    "ci_report": "jenkins/backstop_data/ci_report"
  },
  "report": ["browser"],
  "engine": "puppeteer",
  "engineOptions": {
    "args": ["--no-sandbox"]
  },
  "asyncCaptureLimit": 5,
  "asyncCompareLimit": 50,
  "debug": false,
  "debugWindow": false
}
